import openai

openai.api_key = 'your_api_key'

prompt = """
Item: Frozen Chicken Breast
Actual Sales: 8,500 units
Forecast Sales: 10,000 units
Difference: -15%
Time Period: June 2025

Write a short performance comment for the report:
"""

response = openai.ChatCompletion.create(
    model="gpt-4",
    messages=[{"role": "user", "content": prompt}],
    temperature=0.5,
    max_tokens=100
)

print(response['choices'][0]['message']['content'])


